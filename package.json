  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.0",
    "@snout/eslint-config": "^3.2.2",
    "@snout/jest-config": "^2.2.0",
    "@snout/tsconfig": "^2.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.1",
    "size-limit": "^8.0.0",
    "typescript": "^4.2.3"
